<%= simple_form_for(@survey) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :decision_id %>

    <%= f.input :a_element_id %>

    <%= f.input :b_element_id %>

    <%= f.input :a_value %>

    <%= f.input :b_value %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
